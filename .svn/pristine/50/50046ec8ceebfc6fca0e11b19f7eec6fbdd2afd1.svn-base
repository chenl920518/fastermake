	/*选者框样式切换*/
	$('.checkbox label').click(function() {
		if ($('label em').css('display') == 'block') {
			$('label em').css({
				'display' : 'none'
			});
		} else {
			$('label em').css({
				'display' : 'block'
			});
		}
	})
	
	/* 资格认证多选切换*/
