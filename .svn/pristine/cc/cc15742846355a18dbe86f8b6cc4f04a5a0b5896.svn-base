<script>
	/*导航栏鼠标经过效果*/
	$('.inquiry_detail .section nav li a').hover(
			function() {
				$(this).addClass('active').parent().siblings('li')
						.children('a').removeClass('active');
			})

	//*顶部鼠标经过子信息出现效果*/
	$('.header_top_in .xxs').hover(function() {
		$(this).children('.xx').css({
			'border-color' : 'transparent',
			'border-bottom-color' : 'transparent',
			'background-color' : '#0046cd'
		})
		$(this).children('.xxlist').show();
	}, function() {
		$(this).children('.xx').css({
			'border-color' : 'transparent',
			'background-color' : 'transparent'
		})
		$(this).children('.xxlist').hide();
	})
	/*更多消息里面鼠标经过效果*/
	$('.xxlist a').hover(function() {
		$(this).css({
			'color' : '#006DCC'
		}).siblings().css({
			'color' : '#666'
		})
	})
	
	$('.xxlist .home_head_dl a').hover(function() {
		$(this).css({
			'background-color':'#006DCC',
			'color' : '#fff'
		})
	},function(){
		$('.xxlist .home_head_dl a').css({
			'background-color':'#fff',
			'border':'1px solid #006DCC',
			'color' : '#006DCC'
		})
		
	})
	
	
	   /*语言改变设置*/        
   		function change_lan(){
        	
			  var lan = getCookie("language");
				if(lan == 'en'){
					lan = 'zh'
				}else{
					lan = 'en'
				}
		
              $.post("/account/changeLanguage.do",
        			{"language": lan},    	
        			function(result){
      	        	var url = window.location.search;        	
      	        	if(lan == 'en'){
      	        		url = url.replace('zh','en');
    		            window.location = url;      	        		 
      	        	}else{
      	        		url = url.replace('en','zh');
  	        			window.location = url;
  	        		}
        	});   			

   		}
	

	/*顶部向下箭头点击变成向上箭头
	$('.header_top_in .head_hover').hover(function() {
		$(this).css({
			'background-color' : '#fff'
		}).find('.iconf').css({
			'background' : 'url(/images/xs.png) no-repeat'
		});
	}, function() {
		$(this).css('background-color', 'transparent');
		$(this).find('.iconf').css({
			'background' : 'url(/images/xx.png) no-repeat'
		});
	})*/

	$('.beforLogin').show()
	$('.afterLogin').hide()
	var userName = getCookie("userName");
	var factoryInfo = getCookie("F_INFO");
	var factoryType;
	if (userName != null && userName != '' && userName != undefined) {
		var base = new Base64();
		userName = base.decode(userName);
		$('#username').text(userName);
		$('.beforLogin').hide()
		$('.afterLogin').show()
	}
	if (userName != null && userName != '' && userName != undefined) {
		var base = new Base64();
		factoryInfo = base.decode(factoryInfo);
		var strs = factoryInfo.split('&');
		factoryType = strs[strs.length-1];
		if(factoryType == 2){
			$('#supplier_list').find('a').attr('href',"#").css({"background-color":"#ddd"});			
		}else if(factoryType == 1){
			$('#supplier_list').next().find('a').attr('href',"#").css({"background-color":"#ddd"});
		}
	}

	function exit(){
        
		$.post("/account/clearCookie.do",
			function(result){
			if(result.state == 0){
				//根据cookie判断不同语言退出页面
				var	serverName = getServerName(lan);	
				var lan = getCookie("language");
				if(lan == 'en'){
					window.location = serverName+"/en/login.html";
				}else{
					window.location = serverName+"/zh/login.html";
				}
			}			
		})

		
	}

	//计算新消息数量  add by jason  请不要删除，我先注释了
	var newMessage_interval = setInterval(function() {
		
		if(factoryInfo == null || factoryInfo == ''){
			return false;
		}
		
		$.ajax({
					url : "/noteMessage/newMessageCount.do",
					cache : false,
					success : function(data) {
					
						if(data.state==0){
							
							var summap = data.data
							
							if(summap){
								
								var count = summap.unRead
								var award = summap.award
								var updateProcess = summap.updateProcess
								$('#unRead').text(count?count:"")
								$('#newMessageCount').text(count?count:"")
								$('#inodeNewMessageCount').text(count?count:"")
								$('#award').text(award?award:"")
								$('#inodeNewAwardCount').text(award?award:"")
								$('#updateProcess').text(updateProcess?updateProcess:"")
								$('#inodeNewUpdateProcessCount').text(updateProcess?updateProcess:"")
								
							}
						
						}
						
						
					}
				});
	}, 60000);
	
	
</script>
